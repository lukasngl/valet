# Azure credentials for integration tests
# Copy this to .env and fill in the values

# Service principal for the integration test app
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_TENANT_ID=

# Object IDs of test applications (from Azure Portal > App registrations > Overview)
# The owned app must have the integration test service principal as an owner
TEST_AZURE_OWNED_APP_OBJECT_ID=
# The other app should NOT have the integration test service principal as an owner
TEST_AZURE_OTHER_APP_OBJECT_ID=
